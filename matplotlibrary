import matplotlib.pyplot as plt
import numpy as p
x=p.array([1,12,6,8])
y=p.array([3,5,8,7])
plt.plot(x,y)
plt.xlabel("girls")
plt.ylabel("boys")


import matplotlib.pyplot as plt
import numpy as p
x=p.array([1,2,3,4])
y=[i ** 2 for i in x]
plt.xlabel("numbers")
plt.ylabel("squares")
plt.show()


import matplotlib.pyplot as plt
import numpy as p
x=p.array([1,12,6,8])
y=p.array([3,5,8,7])
plt.plot(x,y)
plt.title("SCHOOL")
plt.xlabel("girls")
plt.ylabel("boys")




import matplotlib.pyplot as plt
import numpy as p
x=p.array([1,2,3,4])
y=[i ** 2 for i in x]
plt.plot(x,y)
plt.xlabel("numbers")
plt.ylabel("squares")
plt.scatter(x,y)
plt.show()


